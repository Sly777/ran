{
  "name": "ran-boilerplate",
  "version": "0.1.0",
  "main": "server.js",
  "scripts": {
    "build": "next build",
    "dev": "node server.js",
    "dev:next": "next",
    "start": "NODE_ENV=production node server.js",
    "start:next": "next start",
    "lint": "eslint --fix '{pages,components,libraries,server}/**/*.js'",
    "bundle:analyze": "ANALYZE_BUNDLE=true yarn dev",
    "bundle:view": "webpack-bundle-analyzer .next/stats.json",
    "lint-staged": "lint-staged",
    "precommit": "lint-staged",
    "setup:before": "npm i shelljs figlet cli-clear",
    "setup": "npm run setup:before && node ./helper_scripts/setup.js && npm run setup:after",
    "setup:after": "npm rm shelljs figlet cli-clear",
    "heroku-postbuild": "next build"
  },
  "dependencies": {
    "chalk": "1.1.3",
    "compression": "1.6.2",
    "express": "4.15.3",
    "graphql": "0.10.3",
    "ip": "1.1.5",
    "isomorphic-fetch": "2.2.1",
    "localforage": "1.5.0",
    "lru-cache": "4.1.1",
    "moment": "2.18.1",
    "next": "beta",
    "next-url-prettifier": "1.1.4",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-apollo": "1.4.2",
    "react-dom": "15.6.1",
    "react-helmet": "5.1.3",
    "react-redux": "5.0.5",
    "redux": "3.7.0",
    "styled-components": "2.0.0",
    "babel-plugin-styled-components": "1.1.4",
    "remote-redux-devtools": "0.5.12"
  },
  "description": "React . Apollo (GraphQL) . Next.js Boilerplate",
  "author": "Ilker Guller <me@ilkerguller.com> (http://ilkerguller.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sly777/ran"
  },
  "bugs": {
    "url": "https://github.com/Sly777/ran/issues"
  },
  "keywords": [
    "next.js",
    "react",
    "graphql",
    "boilerplate",
    "apollo",
    "styled-components",
    "ran",
    "run"
  ],
  "engines": {
    "node": ">=7.6.0",
    "npm": ">=4"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-eslint": "7.2.3",
    "eslint": "4.1.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-config-prettier": "2.2.0",
    "eslint-plugin-import": "2.5.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-prettier": "2.1.2",
    "eslint-plugin-react": "7.1.0",
    "husky": "0.13.4",
    "lint-staged": "4.0.0",
    "ngrok": "2.2.11",
    "nodemon": "1.11.0",
    "pre-commit": "1.2.2",
    "prettier": "1.4.4",
    "webpack-bundle-analyzer": "2.8.2"
  }
}
